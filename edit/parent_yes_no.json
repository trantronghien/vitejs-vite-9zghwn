{
    "type": "group",
    "layout": "vertical",
    "label": "Tiêu chí cha",
    "id": "#childID.1",
    "elements": [
        // thông tin tiêu chí cha
        {
            "id": "#childID.1.1",
            "type": "group",
            "optionType": "group",
            "layout": "horizontal",
            "properties": { // thuộc tính group của tiêu chí cha
                "background": "#FFF3E9"
            },
            "elements": [
                {
                    "id": "#childID.1.1.1",
                    "parentId": "#childID.1.1",
                    "type": "label",
                    "properties": {
                        "value": "Tiêu chí cha", // tiêu đề của tiêu chí cha
                        "fontSize": "16",
                        "fontWeight": "bold"
                    },
                    "rule": [],
                    "elements": []
                },
                {
              "id": "#childID.1.1.2",
              "type": "switch",
              "parentId": "#childID.1.1",
              "properties": {"value": true, "readonly": false, "onLabel": "Yes", "offLabel": "No"},
              "elements": [],
              "rule": [
                {
                  "condition": {
                    "schema": {"value": false},
                    "ref": "#childID.1.2"
                  },
                  "action": "hide" // hide, show #childID.1.2
                },
                {
                  "condition": {
                    "schema": {"value": false},
                    "ref": "#childID.1.2.*"
                  },
                  "action": "notifyDetachElement" // loại toàn bộ #childID.1.2.*, không validate hoặc upload lên server
                },
                {
                  "condition": {
                    "schema": {"value": true},
                    "ref": "#childID.1.2.*"
                  },
                  "action": "notifyAttachElement" // lấy lại toàn bộ #childID.1.2.*
                }
              ]
            } 
            ]
        },
        {
            "id": "#childID.1.2", // cần map
            "parentId": "#childID.1",
            "type": "group",
            "layout": "vertical",
            "elements": [
